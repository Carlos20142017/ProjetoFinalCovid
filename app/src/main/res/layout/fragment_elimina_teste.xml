<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.PessoasFragment.EliminaPessoaFragment" >

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        android:text="@string/temperatura"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/cor_destaque" />

    <TextView
        android:id="@+id/textViewTemperatura"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        tools:text="38"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        android:text="@string/sintomas"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/cor_destaque" />

    <TextView
        android:id="@+id/textViewSintomas"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        tools:text="Nenhum"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        android:text="@string/estado_sa_de"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/cor_destaque" />

    <TextView
        android:id="@+id/textViewEstadoSaude"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        tools:text="Bom"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        android:text="@string/data_do_teste_dd_mm_yyyy"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/cor_destaque" />

    <TextView
        android:id="@+id/textViewDataTeste"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        tools:text="11/05/2021"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        android:text="@string/nome_da_pessoa"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        android:textColor="@color/cor_destaque" />

    <TextView
        android:id="@+id/textViewNomePessoa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_ecran"
        android:layout_marginLeft="@dimen/margin_ecran"
        android:layout_marginTop="@dimen/margin_ecran"
        android:layout_marginEnd="@dimen/margin_ecran"
        android:layout_marginRight="@dimen/margin_ecran"
        tools:text="Sonia"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium" />
</LinearLayout>